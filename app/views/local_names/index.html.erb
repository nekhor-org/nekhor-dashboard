<p style="color: green"><%= notice %></p>

<h1>Local names</h1>

<div id="local_names">
  <% @local_names.each do |local_name| %>
    <%= render local_name %>
    <p>
      <%= link_to "Show this local name", local_name %>
    </p>
  <% end %>
</div>

<%= link_to "New local name", new_local_name_path %>
